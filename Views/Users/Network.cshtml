@model EventRecommender.Controllers.NetworkVm
@{
    ViewData["Title"] = "My network";
}
<h2>My network</h2>

<div class="row">
    <div class="col-md-6">
        <h4>Following</h4>
        <ul>
            @foreach (var n in Model.Following)
            {
                <li>@n</li>
            }
        </ul>
    </div>
    <div class="col-md-6">
        <h4>Followers</h4>
        <ul>
            @foreach (var n in Model.Followers)
            {
                <li>@n</li>
            }
        </ul>
    </div>
</div>
